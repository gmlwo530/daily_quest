

<form action="/home/makeQuest_quest/<%= @userquest.id %>/<%= @user.id %>" method="post" enctype="multipart/form-data">
  <%= hidden_field_tag :authenticity_token, form_authenticity_token %>
  <div class="file-field input-field center waves-red">
    <div class="waves-effect waves-red btn" style="background-color:#fa8072">
      <span>찾아보기</span>
      <input type="file" name="photoData" >
    </div>
    <div class="file-path-wrapper">
      <input class="file-path validate" type="text" placeholder="파일 업로드">
    </div>
  </div>
  <div class="row center" id="target">
      <a href="/home/makeQuest_quest/<%= @userquest.id %>/<%= @user.id %>" id="success"><input type="submit" class="waves-effect waves-light btn" value="제출하기"></a>
      <a href="/home/makeQuest_failed" class="btn red">포기하기</a>
  </div>
</form>
