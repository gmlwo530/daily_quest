    <!--여기서 프로필 상위부문-->
<div style="background-color: #f7f7f7; border-bottom: 2px dotted; color:#0e2f44;">
  <div class="container">
    <div class="row">
      <div class="col s6 m3 l3" style="padding: 30px;">
        <img src="fail3.jpg" class="z-depth-4" width= 100%; height=100%;>
      </div>
      <div class="col s6 m8 l8" style="padding-top: 30px; padding-bottom: 1em; font1size:1.2em;"> 
      <table class="striped centered" style="border-style: double; margin-top:20px">
        <tbody>
          <tr class="blue-grey lighten-4">
            <td>유저이름</td>
            <td>퀘스트 성공률</td>
            <td>성공한 퀘스트 수</td>
            <td>실패한 퀘스트 수</td>
            <td>자주하는 카데고리</td>
          </tr>
          <tr>
            <td><%= current_user.nickname%></td>
            <td>90%</td>
            <td>90개</td>
            <td>10개</td>
            <td>모험(30%)</td>
          </tr>
        </tbody>
      </table>
      </div>
    </div>
  </div>
</div>

  <!--여기서부터 프로필 하위 부분-->
  <div>
    <div class="row">
      <div class="offset-m5 offset-l6 offset-s2 col s4 m2 l2">
        <select class="browser-default" style="margin-top:1em;">
          <option value="disabled selected">카테고리</option>
          <option value="1">정</option>
          <option value="2">모험</option>
          <option value="3">튜토리얼</option>
          <option value="4">자기계발</option>
          <option value="5">해커톤</option>
        </select>
      </div>

     <form class="col s6 m4 l4">
        <div class="row">
          <div class="input-field suffix">
            <input id="icon_prefix" type="text" class="validate">
            <label for="icon_prefix">미션 찾기</label>
            <a href="#"><i class="material-icons teal-text">search</i></a>
          </div>
        </div>
      </form>

    </div>
  <div id="list_card" class="row">
    <% @userquests.each do |uq| %>
      <div class="col s12 m6 l4">
        <div class="card">
          <div class="card-image">
            <img src="fail4.jpg">
            <span class="card-title"><%= uq.quest.content %></span>
            <a class="btn-floating halfway-fab waves-effect waves-light red"><i class="material-icons">add</i></a>
          </div>
          <div class="card-content">
            <p>I am a very simple card. I am good at containing small bits of information. I am convenient because I require little markup to use effectively.</p>
          </div>
        </div>
      </div>
    <% end %>
  </div>

      <div class="center">

    <%= paginate @userquests %>
    </div>
    
  </div>
</div>


<div id="test">
  
  
</div>
  <script>
      $(document).ready(function(){
        $.ajax(url: "/home/bbs").done (html) ->
        $("#test").append html
      })
      
  </script>
    
    