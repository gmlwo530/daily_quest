<!DOCTYPE html>
<html>
<head>
  <title>Daily Quest</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
   <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">
</head>
<body>
  <div>
   <nav>
    <div class="nav-wrapper grey darken-3">
      <a href="/index" class="brand-logo"><img src="<%= asset_path '/DQ.logo_3(white).png' %>" style ="height:40px; padding-top:15px; padding-left:15px; " ></a>
      <a href="#" data-activates="mobile-demo" class="button-collapse"><i class="material-icons">menu</i></a>
      <ul class="right hide-on-med-and-down">
        <li><a href="/makeQuest_success">makeQuest_success</a></li>
        <li><a href="/makeQuest_quest">makeQuest_quest</a></li>
        <li><a href="/makeQuest_new">makeQuest_new</a></li>
        <% if user_signed_in? %>
        <li><a href="/"><%= current_user.id%> </a></li>
        <li><a href="/users/sign_out" data-method="delete">logout</a></li>
        <% else %>
        <li><a href="/users/sign_in">login</a></li>
        <% end %>
          
        
      </ul>
      <ul class="side-nav" id="mobile-demo">
